  {
    "env": {
        "node": true,
        "browser": true
    },
    "extends": "airbnb-base",
    "parserOptions": {
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "rules": {
        "import/extensions": 0,
        "no-console": 0,
        "no-alert": 0,
        "no-use-before-define": ["error", { "functions": false }],
        "no-param-reassign": 0,
        "no-undef": 0,
        "global-require": 0,
        "no-empty": 0,
        "consistent-return": 0,
        "no-unused-vars": 0
    }
}